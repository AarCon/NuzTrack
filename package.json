{
  "name": "nuzlocke",
  "author": "Diego Ballesteros Castellanos",
  "description": "Nuzlocke tracker to record encounters with detailed information",
  "version": "4.9.9",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/diballesteros/nuzlocke/issues"
  },
  "nyc": {
    "exclude": [
      "src/serviceWorker.ts",
      "src/serviceWorkerRegistration.ts",
      "src/components/UpdateSW/UpdateSW.tsx",
      "src/index.tsx"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/service-worker.ts",
      "<rootDir>/src/serviceWorkerRegistration.ts",
      "<rootDir>/src/components/UpdateSW/UpdateSW.tsx",
      "<rootDir>/src/index.tsx"
    ]
  },
  "private": true,
  "dependencies": {
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "@smogon/calc": "^0.6.0",
    "html-to-image": "^1.9.0",
    "i18next": "^21.7.1",
    "immer": "^9.0.12",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.16.9",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^8.2.0",
    "react-window": "^1.8.7",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.2",
    "typescript": "4.5.5",
    "zustand": "3.7.2"
  },
  "scripts": {
    "analyze": "source-map-explorer ''build/static/js/*.js'",
    "start": "react-scripts start",
    "httpss": "HTTPS=true react-scripts start",
    "develop:ci": "CHOKIDAR_USEPOLLING=1 react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "e2e": "cypress open",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x} --ignore-pattern *.test.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/instrument-cra": "^1.4.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "cypress": "^9.6.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-localstorage-commands": "^1.7.0",
    "eslint": "8.4.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "source-map-explorer": "^2.5.2"
  }
}
